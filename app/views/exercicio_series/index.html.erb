<p id="notice"><%= notice %></p>

<h1>Exercicio Series</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Quantidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exercicio_series.each do |exercicio_series| %>
      <tr>
        <td><%= exercicio_series.nome %></td>
        <td><%= exercicio_series.quantidade %></td>
        <td><%= link_to 'Show', exercicio_series %></td>
        <td><%= link_to 'Edit', edit_exercicio_series_path(exercicio_series) %></td>
        <td><%= link_to 'Destroy', exercicio_series, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exercicio Series', new_exercicio_series_path %>
